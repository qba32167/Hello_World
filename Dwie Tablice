public class TablicaDwieTablice {

    public static void main(String[] args) throws IOException {

        int[] tab = new int[20];

        for (int i = 0; i < tab.length; i++) {
            tab[i] = i + 1;
        }
        for (int i = 0; i < tab.length; i++) {
            System.out.println(tab[i]);
        }
    }
}
